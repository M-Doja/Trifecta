<% include partials/header %>

    <div class="text-center container lead">
      <h1 class=""><%= title %></h1>
      <p>Welcome to <%= title %>Update Page </p>

    </div>

    <div class="container jumbotron">

      <div class=" text-center">

              <form class="box text-center" action="/dataUpdate/<%= doc.id %>" method="post">
                <h2>Data Update Form</h2>
              <div class="">
                <label for="subject">Subject</label>
                <input class="form-control"type="text" name="subjectUpdate" id="subject" value="<%= doc.subject %>">
              </div>

              <div class="">
                <label for="body">Body</label>
                <input class="form-control"type="text" name="bodyUpdate" id="body" value="<%= doc.body %>">
              </div><br>
              <div class="">

                <button type="submit" class="btn " name="button">Save Editted Data</button>
                <a href="/" class="cancelEditBtn">Cancel</a>
              </div>
            </form>



      </div>
    </div>

<% include partials/footer %>
